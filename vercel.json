{
    "builds": ["builds": [
    {
      "src": "app/config/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9",
        "env": {
          "PIP_NO_CACHE_DIR": "true",
          "PIP_DISABLE_PIP_VERSION_CHECK": "true",
          "PYTHONUNBUFFERED": "true"
        },
        "buildCommand": "apt-get update && apt-get install -y build-essential && pip install -U pip setuptools wheel ruamel.yaml.clib==0.2.6
&& pip install -r requirements.txt"
      }
    }
  ]
}],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/config//wsgi.py"
        }
    ]
}{
  



